<a href="/viewData">View All Data(Table View)</a>

<h1>{{title}}</h1>

<div style="border:1px solid #ccc; padding:15px; margin-bottom:20px;">
  
  <h2>{{nameOrNA data.NAME}}</h2>

  <p><strong>ID:</strong> {{data.id}}</p>
  <p><strong>Host ID:</strong> {{get data "host id"}}</p>
  <p><strong>Host Name:</strong> {{get data "host name"}}</p>
  <p><strong>Host Verified:</strong> {{data.host_identity_verified}}</p>

  <p><strong>Neighbourhood Group:</strong> {{get data "neighbourhood group"}}</p>
  <p><strong>Neighbourhood:</strong> {{data.neighbourhood}}</p>

  <p><strong>Country:</strong> {{data.country}} ({{data.country_code}})</p>

  <p><strong>Room Type:</strong> {{get data "room type"}}</p>

  <p><strong>Construction Year:</strong> {{get data "Construction year"}}</p>

  <p><strong>Price:</strong> {{data.price}}</p>
  <p><strong>Service Fee:</strong> {{get data "service fee"}}</p>

  <p><strong>Minimum Nights:</strong> {{get data "minimum nights"}}</p>
  <p><strong>Number of Reviews:</strong> {{get data "number of reviews"}}</p>
  <p><strong>Last Review:</strong> {{get data "last review"}}</p>
  <p><strong>Reviews per Month:</strong> {{get data "reviews per month"}}</p>
  <p><strong>Review Rating:</strong> {{get data "review rate number"}}</p>

  <p><strong>Listings Count:</strong> {{get data "calculated host listings count"}}</p>
  <p><strong>Availability 365:</strong> {{get data "availability 365"}}</p>

  <p><strong>House Rules:</strong> {{data.house_rules}}</p>
  <p><strong>License:</strong> {{data.license}}</p>
  <p><strong>Property Type:</strong> {{data.property_type}}</p>

  <div>
    <strong>Thumbnail:</strong><br>
    {{#if data.thumbnail}}
      <img src="{{data.thumbnail}}" width="300">
    {{else}}
      N/A
    {{/if}}
  </div>

  <div>
    <strong>Images:</strong><br>
    {{#if data.images}}
      {{#each data.images}}
        <img src="{{this}}" width="200" style="margin:5px;">
      {{/each}}
    {{else}}
      N/A
    {{/if}}
  </div>

</div>
